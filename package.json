{
  "name": "studesq",
  "version": "0.1.0",
  "private": true,
  "description": "Student journey hub - trust-first academic record keeping",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "test": "vitest",
    "test:smoke": "vitest run tests/smoke.test.ts",
    "setup": "npm run db:push && npm run db:seed && tsx scripts/setup-samples.ts"
  },
  "dependencies": {
    "next": "14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "@prisma/client": "^5.22.0",
    "prisma": "^5.22.0",
    "next-auth": "^4.24.10",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@tanstack/react-query": "^5.59.20",
    "react-hook-form": "^7.53.2",
    "zod": "^3.23.8",
    "@hookform/resolvers": "^3.9.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.4",
    "lucide-react": "^0.462.0",
    "date-fns": "^4.1.0",
    "jose": "^5.9.6"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "tailwindcss": "^3.4.15",
    "postcss": "^8.4.49",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.18",
    "vitest": "^2.1.5",
    "@vitejs/plugin-react": "^4.3.3",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
